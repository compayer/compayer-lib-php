{
  "name": "compayer/compayer-sdk-php",
  "homepage": "https://compayer.com",
  "description": "Library for push stat message to Compayer from php-based projects",
  "minimum-stability": "stable",
  "license": "proprietary",
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/compayer/compayer-lib-php.git"
    }
  ],
  "authors": [
    {
      "name": "Evgeniy Strigo",
      "email": "evgeniy.strigo@protocol.one"
    },
    {
      "name": "Vadim Sabirov",
      "email": "vadim.sabirov@protocol.one"
    }
  ],
  "require": {
    "php": ">=5.5",
    "ext-json": "*",
    "guzzlehttp/guzzle": "~6.5",
    "ramsey/uuid": "3.9.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0",
    "mockery/mockery": "^1.3"
  },
  "autoload": {
    "psr-4": {"Compayer\\SDK\\": "src/Compayer"}
  },
  "autoload-dev": {
    "psr-4": {
      "Compayer\\SDK\\Tests\\": "tests"
    }
  }
}
